<tr>
  <td>
    <%= image_tag product.image.tiny %>
  </td>
  <td>
    <h4><%= product.name %></h4>
    <br>
    <h4><%= product.description %></h4>
  </td>
  <td>
    <%= humanized_money_with_symbol price %>
  </td>
  <td>
    <%= quantity %>
  </td>
  <td>
  <%= humanized_money_with_symbol price * quantity %>
  </td>
</tr>
